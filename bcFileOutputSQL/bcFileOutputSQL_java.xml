<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT>
	<HEADER
			AUTHOR="Benoit Courtine"
			COMPATIBILITY="ALL"
			DATA_AUTO_PROPAGATE="false"
			PLATEFORM="ALL"
			RELEASE_DATE="20110315A"
			SERIAL=""
			STARTABLE="false"
			STATUS="ALPHA"
			VERSION="1.2"
	>
		<SIGNATURE/>
	</HEADER>

	<FAMILIES>
		<FAMILY>File/Output</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER
				NAME="FILENAME"
				FIELD="FILE"
				NUM_ROW="10"
				REQUIRED="true"
		>
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.sql"</DEFAULT>
		</PARAMETER>

		<PARAMETER
				NAME="TABLENAME"
				FIELD="TEXT"
				NUM_ROW="30"
				REQUIRED="true"
		>
			<DEFAULT>"table"</DEFAULT>
		</PARAMETER>

		<PARAMETER
				NAME="DATA_ACTION"
				FIELD="CLOSED_LIST"
				NUM_ROW="40"
		>
			<ITEMS DEFAULT="INSERT">
				<ITEM NAME="INSERT" VALUE="INSERT"/>
				<ITEM NAME="DELETE" VALUE="DELETE"/>
				<!-- ImplÃ©mentation ulterieure
				<ITEM NAME="UPDATE" VALUE="UPDATE"/>
				-->
			</ITEMS>
		</PARAMETER>

		<PARAMETER
				NAME="SCHEMA"
				FIELD="SCHEMA_TYPE"
				REQUIRED="true"
				NUM_ROW="50"
		>
			<DEFAULT/>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER
			NAME="CREATE"
			FIELD="CHECK"
			REQUIRED="true"
			NUM_ROW="10"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="APPEND_FILE"
			FIELD="CHECK"
			REQUIRED="true"
			NUM_ROW="10"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
				NAME="ROWSEPARATOR"
				FIELD="TEXT"
				NUM_ROW="20"
		>
			<DEFAULT>"\n"</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="FORCE_ENCODING"
			FIELD="CHECK"
			REQUIRED="true"
			NUM_ROW="30"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="ENCODING"
			FIELD="ENCODING_TYPE"
			NUM_ROW="30"
			REQUIRED="true"
			SHOW_IF="FORCE_ENCODING == 'true'"
		>
			<DEFAULT>"UTF-8"</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION />
	
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/>
	</RETURNS>
</COMPONENT>
