<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.4"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="BENNATI"
    RELEASE_DATE="20170529A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>File/Input</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/> 
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="1" REQUIRED="true">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.odt"</DEFAULT>
    </PARAMETER>
    <PARAMETER FIELD="SCHEMA_TYPE" NAME="SCHEMA" NUM_ROW="3" REQUIRED="true" READONLY="true"> 
        <TABLE>
        <COLUMN NAME="LINE_ODT" TYPE="id_String" LENGTH="1000" />
      </TABLE>
  </PARAMETER>
  
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>

	<PARAMETER NAME="ADVANCED_SEPARATOR" FIELD="CHECK" REQUIRED="true" NUM_ROW="46" >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="60"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
    
  </ADVANCED_PARAMETERS>

  <CODEGENERATION> 
    <IMPORTS> 
      <IMPORT MODULE="jOpenDocument-1.3.jar" NAME="jOpenDocument-1.3" REQUIRED="true"/>  
      <IMPORT MODULE="jdom-1.1.jar" NAME="jdom-1.1" REQUIRED="true"/> 
    </IMPORTS> 
  </CODEGENERATION>  
  
  <RETURNS> 
    <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/> 
  </RETURNS> 
  
</COMPONENT>
