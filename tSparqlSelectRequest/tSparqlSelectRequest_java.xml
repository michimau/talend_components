<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    DATA_AUTO_PROPAGATE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    STARTABLE="true"
	PARTITIONING="AUTO"
    >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Semantic Web</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
    </PARAMETER>
    
    <PARAMETER NAME="VIRTUOSO_ENDPOINT" FIELD="TEXT" SHOW_IF="TRIPLESTORE_TYPE == 'VIRTUOSO'" NUM_ROW="5">
      <DEFAULT>"http://pubmed.bio2rdf.org/sparql"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="EBI_ENDPOINT" FIELD="TEXT" READONLY="true" SHOW_IF="TRIPLESTORE_TYPE == 'EBI'" NUM_ROW="5">
      <DEFAULT>"www.ebi.ac.uk/rdf/services/reactome/sparql"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="LINKEDLIFE_ENDPOINT" FIELD="TEXT" SHOW_IF="TRIPLESTORE_TYPE == 'LINKEDLIFEDATA'" NUM_ROW="5">
      <DEFAULT>"http://linkedlifedata.com/sparql"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="UNIPROT_ENDPOINT" FIELD="TEXT" READONLY="true" SHOW_IF="TRIPLESTORE_TYPE == 'UNIPROT'" NUM_ROW="5">
      <DEFAULT>"http://beta.sparql.uniprot.org/"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="TRIPLESTORE_TYPE" FIELD="CLOSED_LIST" NUM_ROW="10">
      <ITEMS DEFAULT="VIRTUOSO">
        <ITEM NAME="VIRTUOSO" VALUE="virtuoso"/>
        <ITEM NAME="EBI" VALUE="ebi"/>
        <ITEM NAME="UNIPROT" VALUE="uniprot"/>
        <ITEM NAME="LINKEDLIFEDATA" VALUE="linkedlifedata"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="55"
      REQUIRED="true"
    >
      <DEFAULT>"SELECT count(*)
WHERE
{
	?s ?p ?o .
}"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
   
	   <PARAMETER NAME="CHECK_DISPLAY_URL" FIELD="CHECK" NUM_ROW="10">
	      <DEFAULT>false</DEFAULT>
	   </PARAMETER>

   </ADVANCED_PARAMETERS>

  <CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="json-20140107.jar" MODULE="json-20140107.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

  <RETURNS/>

</COMPONENT>