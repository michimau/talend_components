<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"  
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Bio2RDF"
    RELEASE_DATE="20130919"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Semantic Web/Virtuoso</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="false"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:JDBC"
	/>
   
	<PARAMETER
		NAME="TRIPLESTORE"
		FIELD="CLOSED_LIST"
		NUM_ROW="10"
		SHOW="false"
		REPOSITORY_VALUE="TUTORIAL">
		<ITEMS DEFAULT="VIRTUOSO">
			<ITEM NAME="VIRTUOSO" VALUE="VIRTUOSO" />
			<ITEM NAME="SESAME" VALUE="SESAME" />
			<ITEM NAME="JENA" VALUE="JENA" />
		</ITEMS>
	</PARAMETER>  
	
	
    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="15"
      REPOSITORY_VALUE="HOST"
      REQUIRED="true" 
	>
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="20"
      REPOSITORY_VALUE="PORT"
      REQUIRED="true" 
	>
      <DEFAULT>"1111"</DEFAULT>
    </PARAMETER>
    	
	<PARAMETER
      NAME="DATABASE"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="DATABASE"
      REQUIRED="true"
	>
	<DEFAULT>"DB"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
	>
      <DEFAULT>"dba"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true" 
	>
      <DEFAULT>"dba"</DEFAULT>
    </PARAMETER>
	

	<PARAMETER
      NAME="DATASOURCE_ALIAS"
	  FIELD="TEXT"
      NUM_ROW="65"
      REQUIRED="true"
	  GROUP="DATASOURCE"
	  SHOW_IF="(SPECIFY_DATASOURCE_ALIAS=='true' AND USE_SHARED_CONNECTION=='false')"
    >      
	<DEFAULT>""</DEFAULT>
    </PARAMETER>
	
   </PARAMETERS>

   <ADVANCED_PARAMETERS>
	<PARAMETER
      	NAME="ENCODING"
      	FIELD="ENCODING_TYPE"
      	NUM_ROW="10"
      	REQUIRED="true"
      	SHOW="false"
    >
      	<DEFAULT>"ISO-8859-15"</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="USE_CURSOR" FIELD="CHECK" NUM_ROW="20">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="CURSOR_SIZE" FIELD="TEXT" NUM_ROW="20"
			SHOW_IF="(USE_CURSOR == 'true')">
			<DEFAULT>1000</DEFAULT>
	</PARAMETER>

	
	
	<PARAMETER NAME="TRIM_ALL_COLUMN" FIELD="CHECK" NUM_ROW="30">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="TRIM_COLUMN" FIELD="TABLE" NUM_ROW="40"
		NB_LINES="5" SHOW_IF="TRIM_ALL_COLUMN == 'false'">
		<ITEMS BASED_ON_SCHEMA="true">
			<ITEM NAME="TRIM" FIELD="CHECK"></ITEM>
		</ITEMS>
	</PARAMETER>		
   </ADVANCED_PARAMETERS>

   <CODEGENERATION>
	   <IMPORTS>
	      <IMPORT NAME="virtjdbc3" MODULE="virtjdbc3.jar" REQUIRED="true" />
	   </IMPORTS>
   </CODEGENERATION>

   <RETURNS/>
</COMPONENT>